{
    "name": "unplugin-next-app-router",
    "version": "1.0.0",
    "description": "File-based routing, similar to SvelteKit and Next.js App Router.",
    "keywords": [
        "unplugin",
        "vite",
        "webpack",
        "rollup",
        "types",
        "typed",
        "react-router"
    ],
    "homepage": "https://github.com/zjy040525/unplugin-next-app-router#readme",
    "bugs": {
        "url": "https://github.com/zjy040525/unplugin-next-app-router/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/zjy040525/unplugin-next-app-router.git"
    },
    "license": "MIT",
    "type": "module",
    "exports": {
        ".": {
            "types": {
                "require": "./dist/index.d.cts",
                "import": "./dist/index.d.ts"
            },
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        },
        "./vite": {
            "types": {
                "require": "./dist/vite.d.cts",
                "import": "./dist/vite.d.ts"
            },
            "require": "./dist/vite.cjs",
            "import": "./dist/vite.js"
        },
        "./webpack": {
            "types": {
                "require": "./dist/webpack.d.cts",
                "import": "./dist/webpack.d.ts"
            },
            "require": "./dist/webpack.cjs",
            "import": "./dist/webpack.js"
        },
        "./rollup": {
            "types": {
                "require": "./dist/rollup.d.cts",
                "import": "./dist/rollup.d.ts"
            },
            "require": "./dist/rollup.cjs",
            "import": "./dist/rollup.js"
        },
        "./esbuild": {
            "types": {
                "require": "./dist/esbuild.d.cts",
                "import": "./dist/esbuild.d.ts"
            },
            "require": "./dist/esbuild.cjs",
            "import": "./dist/esbuild.js"
        },
        "./types": {
            "types": {
                "require": "./dist/types.d.cts",
                "import": "./dist/types.d.ts"
            },
            "require": "./dist/types.cjs",
            "import": "./dist/types.js"
        },
        "./client": {
            "types": "./client.d.ts"
        }
    },
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "typesVersions": {
        "*": {
            "./client": [
                "./client.d.ts"
            ],
            "*": [
                "./dist/*",
                "./*"
            ]
        }
    },
    "files": [
        "dist",
        "client.d.ts"
    ],
    "scripts": {
        "build": "tsc --noEmit --skipLibCheck && tsup",
        "build:fix": "esno scripts/postbuild.ts",
        "dev": "tsup --watch src",
        "format": "prettier --write .",
        "prepublishOnly": "npm run build",
        "release": "bumpp && npm publish",
        "start": "esno src/index.ts",
        "test": "vitest",
        "vite6": "npm -C examples/vite6 run dev"
    },
    "dependencies": {
        "fast-glob": "^3.3.2",
        "module-from-string": "^3.3.1",
        "typescript": "^5.5.4",
        "unplugin": "^2.0.0"
    },
    "devDependencies": {
        "@types/node": "^22.5.1",
        "bumpp": "^9.5.2",
        "chalk": "^5.3.0",
        "esno": "^4.7.0",
        "prettier": "3.4.2",
        "prettier-plugin-organize-imports": "4.1.0",
        "prettier-plugin-packagejson": "2.5.6",
        "tsup": "^8.2.4",
        "vitest": "^2.1.8"
    },
    "peerDependencies": {
        "esbuild": "*",
        "react-router": "^7",
        "rollup": "^3",
        "vite": ">=3",
        "webpack": "^4 || ^5"
    },
    "peerDependenciesMeta": {
        "esbuild": {
            "optional": true
        },
        "rollup": {
            "optional": true
        },
        "vite": {
            "optional": true
        },
        "webpack": {
            "optional": true
        }
    },
    "packageManager": "pnpm@9.14.4",
    "volta": {
        "node": "20.18.1",
        "pnpm": "9.14.4"
    }
}
